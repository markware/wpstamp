    top: 0;
    background: #fff;
    box-shadow: 2px 5px 30px rgba(0, 0, 0, 0.4);
    bottom: 0;
    vertical-align: middle;
 
}
   .modal.show{
   	z-index: 9999;
   	display: table;
   	animation:zoomInDown 0.4s;
   	&:before{
   		 position: fixed;
  top:0;
  left:0;
  content: '';
  width: 100%;
  height: 100%;
  z-index: -1;
  background: rgba(0,0,0,.5);
    opacity: 1;
  visibility: visible;
   	}
   }

