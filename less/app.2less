  [data-tooltip]:after {
  	background:desaturate(@secondary-color ,10%);
  	border-radius:@border-radius;
  	bottom: 120%;
  	color: contrast(@secondary-color);
  	content: attr(data-tooltip);
  	font-size: 0.9em;
  	left: 0%;
  	line-height: 1.2;
	animation:fadeIn 0.7s;
  	padding: 0.4em 0.5em;
  	position: absolute;
  	text-align: center;
  	visibility: hidden;
  	text-shadow:none;
  	display: none;
  	z-index: 1;

  }
  
  [data-tooltip]:hover:after, [data-tooltip]:hover:before {
  	visibility: visible; 
  	display: block;

  	 @media @mobile {
  	  	display: none;
  	  }
  	   @media @tablet {
  	  	display: none;
  	  }
  }
